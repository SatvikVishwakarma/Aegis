{
  "Server": {
    "ApiUrl": "http://localhost:8000/api/v1",
    "ApiKey": "YOUR_AGENT_API_KEY_FROM_SERVER_ENV"
  },
  "Node": {
    "Hostname": "auto",
    "IpAddress": "auto",
    "Group": "Windows-Production"
  },
  "Monitoring": {
    "HeartbeatIntervalSeconds": 60,
    "EventBatchSize": 10,
    "EventBatchIntervalSeconds": 5,
    "StatusCheckIntervalSeconds": 30
  },
  "Collectors": {
    "ProcessMonitor": {
      "Enabled": true,
      "ScanIntervalSeconds": 10
    },
    "NetworkMonitor": {
      "Enabled": true,
      "ScanIntervalSeconds": 30
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  }
}
